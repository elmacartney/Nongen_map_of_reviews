@ARTICLE{Grueber20181108,
author={Grueber, C.E. and Gray, L.J. and Morris, K.M. and Simpson, S.J. and Senior, A.M.},
title={Intergenerational effects of nutrition on immunity: a systematic review and meta-analysis},
journal={Biological Reviews},
year={2018},
volume={93},
number={2},
pages={1108-1124},
doi={10.1111/brv.12387},
note={cited By 3},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85035081802&doi=10.1111%2fbrv.12387&partnerID=40&md5=a1b1ecdb00abe48b54f6a334e9b8db57},
